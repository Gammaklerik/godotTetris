piece_land_sfx="*res://assets/SFX/piece_land_sfx.tscn"
leaderboard="*res://leaderboard.tscn"
Settings="*res://assets/SFX/settings.tscn"

[display]

window/size/viewport_width=336
window/size/viewport_height=336
window/size/mode=2
window/stretch/mode="canvas_items"

[editor]

