config_version=5

[application]

config/name="Tetris"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

bg_music="*res://background_music.tscn"
level_up_sfx="*res://assets/SFX/level_up_sfx.tscn"
